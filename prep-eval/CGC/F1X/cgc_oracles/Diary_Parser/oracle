	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p15 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p15.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p15.out"
	;;
