#!/bin/bash
i=Diary_Parser
bin=$(realpath -- ./build/$i/$i)
TIMEOUT=5
if [[ ! -z $ANGELIX_RUN ]]; then TIMEOUT=50; fi; 
TMP=/tmp/angelix/${i}_$RANDOM
mkdir -p $(dirname -- $TMP)
assert-equal () {
	timeout --preserve-status -k $TIMEOUT $TIMEOUT $ANGELIX_RUN $1 < $2  > $TMP
	x=$?
	if (( $x >= 64 )); then rm $TMP ; exit $x; fi; 
	bc=$(wc -c $3)
	cat $TMP | head -c $bc > $TMP.bc
	diff -q $TMP.bc $3 > /dev/null
	xx=$?
	rm $TMP $TMP.bc
	exit $xx
}
case "$1" in 
1)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.n1 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/n1.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/n1.out"
	;;
2)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.n2 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/n2.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/n2.out"
	;;
3)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.n3 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/n3.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/n3.out"
	;;
4)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p1 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p1.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p1.out"
	;;
5)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p2 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p2.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p2.out"
	;;
6)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p3 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p3.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p3.out"
	;;
7)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p4 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p4.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p4.out"
	;;
8)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p5 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p5.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p5.out"
	;;
9)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p6 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p6.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p6.out"
	;;
10)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p7 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p7.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p7.out"
	;;
11)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p8 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p8.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p8.out"
	;;
12)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p9 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p9.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p9.out"
	;;
13)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p10 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p10.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p10.out"
