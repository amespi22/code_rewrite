	;;
38)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p35 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p35.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p35.out"
	;;
39)
