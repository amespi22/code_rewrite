	;;
40)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p37 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p37.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p37.out"
	;;
41)
	export seed=$(cat /mnt/Data/APR_eval/cgc_oracles/Diary_Parser/seed.p38 | head -n1)
	assert-equal "$bin" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p38.in" "/mnt/Data/APR_eval/cgc_oracles/Diary_Parser/p38.out"
